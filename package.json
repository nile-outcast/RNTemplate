{
  "name": "rick_and_morty",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && yarn react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pods": "cd ./ios && pod install && cd ..",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix -c .eslintrc.js",
    "build:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "clear:watch": "watchman watch-del '/Users/nazil/Codes/MOBILE/RNTemplate' && watchman watch-project '/Users/nazil/Codes/MOBILE/RNTemplate'",
    "codegen": "graphql-codegen --config src/apollo/codegen.ts",
    "codegen:watch": "graphql-codegen --watch --config src/apollo/codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "3.4.17",
    "@react-native-voice/voice": "3.2.2",
    "@react-navigation/bottom-tabs": "6.0.7",
    "@react-navigation/native": "6.0.4",
    "@react-navigation/native-stack": "6.2.2",
    "graphql": "16.0.1",
    "react": "17.0.2",
    "react-native": "0.66.5",
    "react-native-modal": "13.0.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "3.8.0",
    "react-native-svg": "12.1.1",
    "styled-components": "5.3.1",
    "styled-tools": "1.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/runtime": "7.12.5",
    "@graphql-codegen/cli": "3.2.1",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript": "3.0.1",
    "@graphql-codegen/typescript-operations": "3.0.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.23",
    "@types/react-native": "0.65.0",
    "@types/react-test-renderer": "17.0.1",
    "@types/styled-components": "5.1.14",
    "@types/styled-components-react-native": "5.1.1",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.66.0",
    "react-test-renderer": "17.0.2",
    "typescript": "4.4.3"
  },
  "resolutions": {
    "@types/react": "17",
    "react-devtools-core": "4.26.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
